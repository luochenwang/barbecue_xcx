.tt {
  line-height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  color: #696969;
  font-weight: bold;
  border-bottom: 1rpx solid #f14958;
}
.container-nav .banner {
  margin-bottom: 75rpx;
}
.container-nav .tab-nav {
  position: fixed;
}
.tab-nav {
  height: 75rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  margin-bottom: 50rpx;
  left: 0;
  width: 100%;
  z-index: 10;
}
.tab-nav .item {
    width: 100%;
    font-size: 24rpx;
    color: #696969;
    background: #eeeeee;
    border-right: 1rpx solid #898989;
    line-height: 75rpx;
    text-align: center;
}
.tab-nav .item:last-child {
      border-right: 0;
}
.tab-nav .item.active {
      background: #fff;
      border-bottom: 1rpx solid #898989;
      border-right: 0;
}
.pro-tt {
  width: 690rpx;
  margin: 0 auto 50rpx;
  position: relative;
  font-size: 28rpx;
  font-weight: bold;
  color: #696969;
  text-align: center;
}
.pro-tt:before {
    content: '';
    position: absolute;
    top: 50%;
    left: 0;
    width: 35%;
    height: 1rpx;
    background: #898989;
}
.pro-tt:after {
    content: '';
    position: absolute;
    top: 50%;
    right: 0;
    width: 35%;
    height: 1rpx;
    background: #898989;
}
.pro-info {
  margin-bottom: 50rpx;
}
.pro-info .video-box {
    width: 670rpx;
    height: 385rpx;
    margin: 0 auto 35rpx;
}
.pro-info .video-box video {
      width: 100%;
      height: 100%;
}
.pro-info .info {
    width: 650rpx;
    margin: 0 auto 35rpx;
    font-size: 24rpx;
    color: #696969;
}
.pro-info .img-box {
    width: 670rpx;
    margin: 0 auto 35rpx;
}
.pro-info .img-box image {
      width: 100%;
      height: 100%;
}
.pro-info .list {
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-wrap: wrap;
        -ms-flex-wrap: wrap;
            flex-wrap: wrap;
}
.pro-info .list .item {
      width: 50%;
      padding: 0 30rpx;
      margin-bottom: 30rpx;
}
.pro-info .list .item .img-box {
        width: 100%;
        margin-bottom: 30rpx;
}
.pro-info .list .item .img-box image {
          width: 100%;
}
.pro-info .list .item .txt {
        font-size: 24rpx;
        text-align: center;
}

