.tt {
  line-height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  color: #696969;
  font-weight: bold;
  border-bottom: 1rpx solid #f14958;
}
.banner swiper {
  height: 420rpx;
}
.banner image {
  width: 100%;
}
.container-nav .banner {
  margin-bottom: 75rpx;
}
.container-nav .tab-nav {
  position: fixed;
}
.tab-nav {
  height: 75rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  margin-bottom: 50rpx;
  /*position: fixed;*/
  left: 0;
  width: 100%;
  z-index: 10;
}
.tab-nav .item {
    width: 100%;
    font-size: 24rpx;
    color: #696969;
    background: #eeeeee;
    line-height: 75rpx;
    text-align: center;
}
.tab-nav .item:last-child {
      border-right: 0;
}
.tab-nav .item.active {
      background: #fff;
      border-right: 0;
}
.pro-tt {
  width: 690rpx;
  margin: 0 auto 50rpx;
  position: relative;
  font-size: 28rpx;
  font-weight: bold;
  color: #696969;
  text-align: center;
}
.pro-tt:before {
    content: '';
    position: absolute;
    top: 50%;
    left: 0;
    width: 35%;
    height: 1rpx;
    background: #898989;
}
.pro-tt:after {
    content: '';
    position: absolute;
    top: 50%;
    right: 0;
    width: 35%;
    height: 1rpx;
    background: #898989;
}
.swiper-box {
  padding-bottom: 50rpx;
}
.pro-info {
  margin-bottom: 50rpx;
}
.pro-info .video-box {
    width: 670rpx;
    height: 380rpx;
    margin: 0 auto 35rpx;
}
.pro-info .video-box video {
      width: 100%;
      height: 100%;
}
.pro-info swiper {
    height: 600rpx;
}
.pro-info .info {
    width: 650rpx;
    margin: 0 auto 35rpx;
    font-size: 24rpx;
    color: #696969;
}
.pro-info .img-box {
    width: 670rpx;
    margin: 0 auto 35rpx;
}
.pro-info .img-box image {
      width: 100%;
      height: 100%;
}
.pro-list {
  margin-bottom: 50rpx;
}
.pro-list .list {
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-wrap: wrap;
        -ms-flex-wrap: wrap;
            flex-wrap: wrap;
}
.pro-list .list .item {
      width: 50%;
      padding: 0 30rpx;
      margin-bottom: 30rpx;
      -webkit-box-sizing: border-box;
              box-sizing: border-box;
}
.pro-list .list .item .img-box {
        width: 100%;
        margin-bottom: 30rpx;
}
.pro-list .list .item .img-box image {
          width: 100%;
}
.pro-list .list .item .txt {
        font-size: 24rpx;
        text-align: center;
}
.data-list {
  margin-bottom: 50rpx;
}
.data-list .list {
    width: 620rpx;
    margin: 0 auto;
    border: 1rpx solid #696969;
    border-radius: 20rpx;
    padding: 20rpx;
}
.data-list .list .item {
      position: relative;
      margin-bottom: 30rpx;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-justify-content: space-between;
          -ms-flex-pack: justify;
              justify-content: space-between;
}
.data-list .list .item .txt {
        color: #696969;
        font-size: 24rpx;
        width: 380rpx;
        text-decoration: underline;
}
.data-list .list .item .icon {
        width: 40rpx;
        height: 40rpx;
}
.data-list .list .item .view {
        background: url(https://campaign5.method-ad.cn/hypertherm/img/show/icon_view1.png) no-repeat 0 0/100% auto;
}
.data-list .list .item .share {
        background: url(https://campaign5.method-ad.cn/hypertherm/img/show/icon_share1.png) no-repeat 0 0/100% auto;
        position: relative;
}
.data-list .list .item .share button {
          position: absolute;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          opacity: 0;
}
.data-list .list .item .download {
        background: url(https://campaign5.method-ad.cn/hypertherm/img/show/icon_download1.png) no-repeat 0 0/100% auto;
}
.how-pay .leads-dialog {
  width: 650rpx;
  background: #fff;
  border-radius: 20rpx;
  padding: 45rpx 35rpx 50rpx;
  position: relative;
  margin: 0 auto;
}
.how-pay .leads-dialog .form-group {
    margin-bottom: 25rpx;
    position: relative;
}
.how-pay .leads-dialog .form-group.icon-arrow:after {
      content: '';
      position: absolute;
      bottom: 5rpx;
      right: 20rpx;
      width: 0;
      height: 0;
      border: 20rpx solid transparent;
      border-top-color: black;
      pointer-events: none;
}
.how-pay .leads-dialog .form-group label {
      font-size: 24rpx;
      display: block;
      margin-bottom: 10rpx;
}
.how-pay .leads-dialog .form-group input, .how-pay .leads-dialog .form-group .txt {
      width: 100%;
      height: 75rpx;
      line-height: 75rpx;
      font-size: 30rpx;
      padding: 0 20rpx;
      border: 1Px solid #000;
      -webkit-box-sizing: border-box;
              box-sizing: border-box;
}
.how-pay .leads-dialog .form-group textarea {
      width: 100%;
      height: 210rpx;
      font-size: 30rpx;
      padding: 20rpx;
      border: 1Px solid #000;
      -webkit-box-sizing: border-box;
              box-sizing: border-box;
}
.how-pay .leads-dialog .privacy {
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    margin-bottom: 10rpx;
}
.how-pay .leads-dialog .privacy text {
      font-size: 22rpx;
      white-space: nowrap;
}
.how-pay .leads-dialog .privacy navigator {
      font-size: 22rpx;
      color: #121dff;
      border-bottom: 1Px solid #121dff;
      white-space: nowrap;
}
.how-pay .leads-dialog .tips {
    color: #696969;
    font-size: 22rpx;
    margin: 20rpx 0;
}
.how-pay .leads-dialog .label {
    font-size: 22rpx;
    margin-bottom: 10rpx;
}
.how-pay .leads-dialog .submit {
    margin: 80rpx auto;
    width: 260rpx;
    line-height: 70rpx;
    text-align: center;
    border-radius: 70rpx;
    color: #fff;
    background: #ed1b2e;
}
.how-pay .leads-dialog .footer {
    margin-bottom: 20rpx;
}
.how-pay .leads-dialog .copyright {
    font-size: 21rpx;
}

